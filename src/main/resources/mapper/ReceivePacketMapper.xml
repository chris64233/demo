<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.mapper.ReceivePacketMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.demo.dao.entity.ReceivePacket">
                <id column="id" property="id" />
                <result column="ue_sn" property="ueSn" />
                <result column="report_time" property="reportTime" />
                <result column="packet_type" property="packetType" />
                <result column="packet" property="packet" />
                <result column="response_packet" property="responsePacket" />
                <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, ue_sn, report_time, packet_type, packet, response_packet, create_time
    </sql>

</mapper>