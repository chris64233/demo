<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.code_generator.mapper.BankAccountMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.demo.code_generator.entity.BankAccount">
        <id column="id" property="id" />
        <result column="mer_no" property="merNo" />
        <result column="account_no" property="accountNo" />
        <result column="account_name" property="accountName" />
        <result column="account_type" property="accountType" />
        <result column="bank_name" property="bankName" />
        <result column="interbank_no" property="interbankNo" />
        <result column="create_time" property="createTime" />
        <result column="modify_time" property="modifyTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, mer_no, account_no, account_name, account_type, bank_name, interbank_no, create_time, modify_time
    </sql>

</mapper>
